{
  "name": "react-quickstart-template",
  "version": "1.0.0",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "format-check": "prettier '{*,{src}/**/*}.{ts,tsx,js,jsx,html,json,css,yml}' --check",
    "lint-check": "tslint --project tsconfig.json --config tslint.json '{src}/**/*.{ts,tsx}'",
    "check": "npm run format-check && npm run type-check && npm run lint-check",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-update": "export NODE_ENV='dev' && yarn install && rm -r .next | true && export NODE_ENV='production' && yarn run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^10.17.13",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-define": "^2.0.0",
    "css-loader": "^1.0.1",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "html-webpack-plugin": "3.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "ts-lint": "^4.5.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.7.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "dotenv": "^8.2.0",
    "highcharts": "^8.0.0",
    "highcharts-react-official": "^2.2.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.2.1",
    "next-fonts": "^1.0.3",
    "next-images": "^1.3.0",
    "nodemailer": "^6.4.5",
    "pg": "^7.18.1",
    "react": "^16.12.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.12.0",
    "serve-index": "^1.9.1",
    "styled-components": "^4.4.1",
    "uuid": "^7.0.2"
  }
}
